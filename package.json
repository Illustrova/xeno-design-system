{
  "name": "@expat/xeno-design-system",
  "version": "1.0.0",
  "description": "Design system for expat.gr project",
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    }
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o build",
    "tokens": "yarn tokens:transform && yarn tokens:style-dictionary && prettier --write \"./src/tokens/build\" && yarn tokens:clean",
    "tokens:style-dictionary": "ts-node ./src/tokens/config.ts",
    "tokens:transform": "token-transformer ./src/tokens/tokens.json ./src/tokens/converted.json --expandTypography=true --throwErrorWhenNotResolved=true",
    "tokens:clean": "rimraf ./src/tokens/converted.json",
    "lint": "eslint -c .eslintrc.json './src/**/*.ts{,x}'",
    "lint:ci": "eslint-output",
    "lint:fix": "eslint --cache --fix -c .eslintrc.json './src/**/*.ts{,x}'",
    "lint:staged": "eslint --cache --fix -c .eslintrc.json",
    "lint:types": "tsc --project tsconfig.lint.json",
    "test": "jest",
    "test:ci": "jest --ci --testTimeout=15000 --reporters=default --reporters=jest-junit --coverage",
    "prepare": "husky install"
  },
  "author": "Irina Illustrova",
  "license": "ISC",
  "lint-staged": {
    "*.ts{,x}": [
      "yarn lint:staged",
      "prettier --write"
    ]
  },
  "resolutions": {
    "@testing-library/dom": "8.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addons": "^6.5.6",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-react": "^1.3.0",
    "@swc/core": "^1.3.11",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "16.11.66",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "1.1.4",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-output": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-junit": "^14.0.1",
    "lint-staged": "^12.3.4",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "storybook-addon-designs": "6.3.1",
    "style-dictionary": "^3.7.0",
    "token-transformer": "^0.0.27",
    "ts-jest": "^29.0.3",
    "ts-loader": "8.2.0",
    "ts-node": "^10.8.0",
    "tslib": "^2.2.0",
    "typescript": "^4.8.4",
    "vite": "2.7.13",
    "vite-plugin-svgr": "^2.2.2"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8"
  },
  "peerDependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "^5.4.4",
    "@mui/system": "^5.4.4",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
