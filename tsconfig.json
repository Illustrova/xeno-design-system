{
  "compilerOptions": {
    "lib": ["es2017"],
		"module": "commonjs",
     "esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitOverride": true,
		"noPropertyAccessFromIndexSignature": false,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
    "noEmit": true
	},
	"include": ["**/*.ts"],

}
